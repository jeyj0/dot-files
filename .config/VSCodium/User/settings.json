{
    "vscode-neovim.mouseSelectionStartVisualMode": false,
    "vscode-neovim.neovimPath": "/run/current-system/sw/bin/nvim",
    "workbench.colorTheme": "Dark (Molokai)",
    "editor.renderControlCharacters": false,
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "editor.lineNumbers": "relative",
    "editor.fontFamily": "'Hack','Fira Code','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 15.5,
    "editor.renderWhitespace": "none",
    "window.menuBarVisibility": "toggle",
    "workbench.activityBar.visible": false,
    "workbench.editor.showTabs": false,
    "editor.renderIndentGuides": false,
    "editor.renderLineHighlight": "all",
    "editor.matchBrackets": "near",
    "workbench.sideBar.location": "left",
    "workbench.startupEditor": "welcomePage",
    "workbench.statusBar.visible": true,
    "workbench.colorCustomizations": {
        "[Dark (Molokai)]": {
            "statusBar.background": "#00000000",
        }
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
}